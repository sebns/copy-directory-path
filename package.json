{
    "name": "copy-directory-path",
    "displayName": "copy-directory-path",
    "description": "Copies path of directory to clipboard",
    "version": "0.0.1",
    "publisher": "sebns",
    "repository": "https://github.com/sebns/copy-directory-path",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.copyDirectoryPath"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.copyDirectoryPath",
                "title": "Copy directory path"
            }
        ],
        "menus": {
            "editor/title/context": [{
                "command": "extension.copyDirectoryPath"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/copy-paste": "^1.1.30",
        "copy-paste": "^1.3.0"
    }
}
